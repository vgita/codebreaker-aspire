{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "DataStore": "Cosmos",
  "Aspire": {
    "Microsoft": {
      "EntityFrameworkCore": {
        "Cosmos": {
          "DbContextPooling": true,
          "Tracing": true,
          "Metrics": true
        },
        "SqlServer": {
          "DbContextPooling": true,
          "Tracing": true,
          "HealthCheck": true,
          "Metrics": true
        }
      }
    },
    "StackExchange": {
      "Redis": {
        "Tracing": true,
        "Metrics": true,
        "HealthCheck": true
      }
    }
  }
}
